[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.2
# We store native structs inside of RivetTask, so we can't reload this plugin.
reloadable = false

[libraries]
linux.editor.x86_64 = "res://addons/rivet/native/librivet_plugin_godot_linux_x86_64.so"
windows.editor.x86_64 = "res://addons/rivet/native/librivet_plugin_godot_windows_x86_64.dll"
macos.editor.x64_64 = "res://addons/rivet/native/librivet_plugin_godot_x86_64.dylib"
macos.editor.arm64 = "res://addons/rivet/native/librivet_plugin_godot_macos_arm64.dylib"
