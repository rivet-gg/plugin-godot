{
	"sdks": [
		{
			"target": "godot",
			"output": "addons/rivet_sdk"
		}
	],
	"modules": {
		"rate_limit": {},
		"tokens": {},
		"lobbies": {
			"config": {
				"lobbies": {
					"regions": [
						"atl",
						"fra",
						"lax"
					],
					"backend": {
						"server": {
							"environment": {
								"SERVER_HOSTNAME": "0.0.0.0"
							},
							"ports": {
								"game": {
									"protocol": "udp",
									"internalPort": 7777
								}
							},
							"resources": {
								"cpu": 500,
								"memory": 500
							}
						}
					}
				}
			}
		},
		"rivet": {
			"config": {
				"apiEndpoint": "https://api.rivet.gg",
				"apiEndpointVariable": "RIVET_API_ENDPOINT",
				"serviceTokenVariable": "RIVET_SERVICE_TOKEN",
				"gameIdVariable": "RIVET_GAME_ID",
				"environmentIdVariable": "RIVET_ENVIRONMENT_ID"
			}
		},
		"users": {},
		"currency": {}
	}
}